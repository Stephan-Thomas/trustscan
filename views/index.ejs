      <!-- header -->
      <header
        class="w-full h-[100px] bg-[#265b20] px-[20px] flex items-center justify-between fixed z-10"
      >
        <!-- logo -->
        <div
          class="logo h-[60px] w-[150px] rounded-[5px] grid place-content-center"
        >
          <h1 class="text-white text-[2rem] font-[600]">Trust Scan</h1>
          <!-- <img
            class="h-[60px] w-[60px]"
            src="https://img.icons8.com/color/48/detective.png"

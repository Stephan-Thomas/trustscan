                <p class="text-white text-center text-[1.2rem]">
                  Transparency and Reliability is assured.
                </p>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- third section -->
      <section
        id="how-it-works"
        class="third-section h-[500px] w-full flex flex-col items-center scroll-margin-[200px]"
      >
        <h2 class="section-title text-[2rem] font-[600] my-[30px]">
          How It Works
        </h2>
        <div
          class="container w-full h-[400px] grid grid-cols-2 grid-rows-2 gap-[20px] border-2"
        >
          <div></div>
          <div></div>
          <div></div>
          <div></div>
        </div>
      </section>
      <section class="contact-us h-[700px] w-full flex flex-col items-center">
        <h2 class="section-title text-[2rem] font-[600] my-[30px]">
          Contact Us
        </h2>
        <p class="text-[#265b20] text-[1.2rem] font-[500] mb-[20px]">
          Have questions or notice anything unusual, reach out to us.
        </p>
        <!-- papa div -->
        <div class="form-container h-[600px] w-full flex px-[30px]">
          <div
            class="w-[70%] h-full bg-[#265b20] flex items-center justify-center"
          >
            <form
              name="form"
              onsubmit="return validateForm()"
              class="form h-[480px] w-[70%] bg-white rounded-[30px] px-[40px] py-[20px] relative"
            >
              <!-- name -->
              <div class="name h-[75px] mb-[15px] w-full">
                <label for="name">Your Full Name</label>
                <input
                  name="name"
                  id="name"
                  placeholder="Jane Doe"
                  type="text"
                  required
                  class="block h-[50px] border-gray-200 border-2 w-full text-[#265b20] focus:border-[#265b20]"
                />
              </div>
              <!-- email -->
              <div class="email h-[75px] mb-[15px] w-full">
                <label for="email">Your Email</label>
                <input
                  id="email"
                  placeholder="JaneDoe@gmail.com"
                  type="text"
                  required
                  class="block h-[50px] border-gray-200 border-2 w-full text-[#265b20] focus:border-[#265b20]"
                />
              </div>
              <!-- message -->
              <div class="name h-[75px] mb-[15px] w-full">
                <label for="name">Your Message</label>
                <textarea
                  placeholder="Your Message Here...."
                  class="h-[150px] w-full border-2 border-gray-200 rounded-[5px] resize-none focus:border-[#265b20] p-[15px] text-[#265b20]"
                ></textarea>
              </div>
              <button
                type="submit"
                class="submit-btn text-white bg-[#5ac44a] h-[50px] w-[150px] font-semibold hover:brightness-90 absolute bottom-[30px] left-[30px]"
              >
                Submit
              </button>
            </form>
          </div>
          <div
            class="w-[30%] h-full rounded-l-[40px] bg-[url(/build/img/contact.jpg)] bg-cover bg-center"
          ></div>
        </div>
      </section>
